{
  "name": "starinc/nex_votes",
  "description": "Holding a voting for Nex.",
  "type": "flarum-extension",
  "keywords": [
    "discussion",
    "voting"
  ],
  "license": "MIT",
  "require": {
    "flarum/core": "^0.1.0-beta.13"
  },
  "replace": {
    "reflar/polls": "^1.3.4"
  },
  "conflict": {
    "reflar/polls": "< 1.3.4"
  },
  "authors": [
    {
      "name": "SuperSonic",
      "email": "supersonic@livemail.tw",
      "role": "Maintainer"
    },
    {
      "name": "David Sevilla Martin",
      "email": "david.s@redevs.org",
      "role": "Original Developer"
    },
    {
      "name": "Shahiem Seymor",
      "role": "Original Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "FoF\\Polls\\": "src/",
      "FoF\\Polls\\Migrations\\": "migrations/"
    }
  },
  "extra": {
    "flarum-extension": {
      "title": "Nex Votes",
      "icon": {
        "name": "fas fa-signal",
        "backgroundColor": "#a403d8",
        "color": "#fff"
      }
    }
  }
}
